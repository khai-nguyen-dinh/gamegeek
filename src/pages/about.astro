---
import LayoutRenius from '../layouts/LayoutRenius.astro';
import Navbar from '../components/Navbar.astro';
---

<LayoutRenius title="About Us | GameGeek">

<style>
  /* Use Inter font for entire page */
  body {
    font-family: 'Inter', sans-serif !important;
  }

  /* Ensure RemixIcon uses correct font - must be after body rule */
  [class^="ri-"], [class*=" ri-"], i[class^="ri-"], i[class*=" ri-"] {
    font-family: 'remixicon' !important;
    font-style: normal !important;
  }

  /* Limit game controller icon size and change color to orange */
  img[src*="game-controller-svgrepo-com.svg"] {
    height: 12px !important;
    width: auto !important;
    max-width: none !important;
    object-fit: contain;
    vertical-align: middle;
    display: inline-block;
    /* Convert to orange color #FF5E14 */
    filter: brightness(0) saturate(100%) invert(48%) sepia(100%) saturate(2000%) hue-rotate(0deg) brightness(1.05) contrast(1.1);
  }

  /* Compact menu when scrolled */
  #navbar.navbar-compact {
    padding: 12px 0;
  }

  #navbar.navbar-compact .navbar-brand,
  #navbar.navbar-compact .other-options {
    display: none !important;
  }

  #navbar.navbar-compact .menu-area {
    flex: 1 !important;
    display: flex !important;
    justify-content: center !important;
  }

  #navbar.navbar-compact .menu-section {
    display: flex !important;
    align-items: center !important;
    gap: 2px !important;
    background: #4a4a4a !important;
    border-radius: 50px !important;
    padding: 6px 18px !important;
    margin: 0 auto !important;
    list-style: none !important;
    box-shadow: 0 2px 12px rgba(0, 0, 0, 0.2) !important;
    flex-direction: row !important;
    flex-wrap: nowrap !important;
    height: auto !important;
  }

  #navbar.navbar-compact .menu-section li {
    margin: 0 !important;
    padding: 0 !important;
    list-style: none !important;
  }

  #navbar.navbar-compact .menu-section li a {
    color: #ffffff !important;
    font-weight: 600 !important;
    font-size: 14px !important;
    padding: 8px 18px !important;
    display: block !important;
    text-decoration: none !important;
    transition: all 0.3s ease !important;
    border-radius: 25px !important;
    white-space: nowrap !important;
    background: transparent !important;
    line-height: 1.5 !important;
    letter-spacing: 0.3px !important;
  }

  #navbar.navbar-compact .menu-section li a:hover {
    background: rgba(255, 255, 255, 0.15) !important;
    color: #ffffff !important;
  }

  /* Responsive adjustments */
  @media (max-width: 991px) {
    #navbar.navbar-compact {
      padding: 10px 0;
    }

    #navbar.navbar-compact .menu-section {
      padding: 5px 14px !important;
      gap: 1px !important;
    }

    #navbar.navbar-compact .menu-section li a {
      padding: 7px 14px !important;
      font-size: 13px !important;
    }
  }

  @media (max-width: 767px) {
    #navbar.navbar-compact {
      padding: 8px 0;
    }

    #navbar.navbar-compact .menu-section {
      padding: 4px 12px !important;
      flex-wrap: wrap;
      justify-content: center;
      gap: 1px !important;
      border-radius: 30px !important;
    }

    #navbar.navbar-compact .menu-section li a {
      padding: 6px 12px !important;
      font-size: 12px !important;
    }
  }

  /* Counter Section Styles */
  .counter-box.style-three {
    padding: 40px 20px;
  }

  .counter-box.style-three .counter {
    margin-bottom: 20px;
  }

  .counter-box.style-three .counter-number {
    font-size: clamp(60px, 8vw, 100px) !important;
    line-height: 1;
    display: inline-block;
    font-weight: 900 !important;
    letter-spacing: -2px;
  }

  .counter-box.style-three .plus {
    font-size: clamp(45px, 6vw, 70px) !important;
    margin-left: 8px;
    font-weight: 700;
  }

  .counter-box.style-three .text {
    font-size: 16px;
    margin-top: 15px;
    line-height: 1.5;
  }

  @media (max-width: 768px) {
    .counter-box.style-three {
      padding: 30px 15px;
    }
    
    .counter-box.style-three .counter-number {
      font-size: clamp(50px, 10vw, 80px) !important;
    }
    
    .counter-box.style-three .plus {
      font-size: clamp(35px, 8vw, 60px) !important;
    }
  }

  /* Brand Promises Quote Icon */
  .feature-area.style-four .quote-icon {
    width: 80px;
    height: 80px;
    min-width: 80px;
  }

  .feature-area.style-four .quote-icon img {
    width: 40px;
    height: 40px;
  }

  @media (max-width: 768px) {
    .feature-area.style-four .quote-icon {
      width: 60px;
      height: 60px;
      min-width: 60px;
    }

    .feature-area.style-four .quote-icon img {
      width: 30px;
      height: 30px;
    }

    .feature-area.style-four .section-title[style*="padding-left"] {
      padding-left: 70px !important;
    }
  }

  /* Brand Partner Logo Height */
  .brand-slider .brand-logo img {
    height: 120px !important;
    width: auto !important;
    object-fit: contain;
  }

  /* Brand Promises Award Cards */
  .feature-area.style-four .award-badge {
    width: 80px;
    height: 80px;
    min-width: 80px;
    flex-shrink: 0;
  }

  .feature-area.style-four .award-badge span {
    display: block;
  }

  .feature-area.style-four .award-title {
    flex: 1;
    padding-left: 20px;
  }

  .feature-area.style-four .award-title p {
    line-height: 1.6;
  }

  @media (max-width: 768px) {
    .feature-area.style-four .award-badge {
      width: 60px;
      height: 60px;
      min-width: 60px;
    }

    .feature-area.style-four .award-badge .fs-36 {
      font-size: 24px !important;
    }

    .feature-area.style-four .award-badge .fs-16 {
      font-size: 12px !important;
    }

    .feature-area.style-four .award-title {
      padding-left: 15px;
    }
  }
</style>

