                Testimonial 
                <img src="/src/assets/images/icon_title.png" alt="icon">
              </div>
              <h2 class="title wow fadeInUp" data-wow-delay=".4s">
                What our clients say?
              </h2>
            </div>
          </div>
          <div class="slider-area testimonialSliderOne">
            <div class="swiper gt-slider" id="testimonialSliderOne">
              <div class="swiper-wrapper cs_slider_gap_301">
                <div class="slick-slider slick-initialized" dir="ltr">
                  <div class="slick-list">
                    <div class="slick-track" style="width: 5880px; opacity: 1; transform: translate3d(-1568px, 0px, 0px); transition: -webkit-transform 2000ms;">
                      <!-- Testimonial 1 -->
                      <div data-index="0" class="slick-slide" tabindex="-1" aria-hidden="true" style="outline: none; width: 392px;">
                        <div>
                          <div class="swiper-slide" tabindex="-1" style="width: 100%; display: inline-block;">
                            <div class="testimonial-card style1">
                              <div class="testimonial-header">
                                <div class="profile-thumb">
                                  <img src="/src/assets/images/testimonialProfileThumb1_1.jpg" alt="thumb">
                                </div>
                                <div class="content">
                                  <h5>Jacob Jones</h5>
                                  <p class="text">Team Leader</p>
                                </div>
                              </div>
                              <div class="testimonial-body">
                                <ul class="star-wrapper style1">
                                  <li><img src="/src/assets/images/starIcon1_1.svg" alt="icon"></li>
                                  <li><img src="/src/assets/images/starIcon1_1.svg" alt="icon"></li>
