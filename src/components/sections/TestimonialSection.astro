<section class="testimonial-section">
  <div class="testimonial-container-wrapper style1">
    <div class="container">
      <div class="testimonial-wrapper style1 section-padding fix">
        <div class="shape1">
          <img src="/src/assets/images/testimonialShape1_1.png" alt="shape">
        </div>
        <div class="shape2">
          <img src="/src/assets/images/testimonialShape1_2.png" alt="shape">
        </div>
        <div class="container">
          <div class="section-title text-center mxw-685 mx-auto">
